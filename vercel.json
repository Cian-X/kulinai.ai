{
  "version": 2,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/index.cjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
